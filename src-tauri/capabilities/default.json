{
  "$schema": "../gen/schemas/desktop-schema.json",
  "identifier": "default",
  "description": "enables the default permissions",
  "windows": [
    "main"
  ],
  "permissions": [
    "core:default",
    "fs:default",
    {
      "identifier": "fs:allow-mkdir",
      "allow": [{ "path": "$DOCUMENT/Mutaba3a/**" }]
    },
    {
      "identifier": "fs:allow-write",
      "allow": [{ "path": "$DOCUMENT/Mutaba3a/**" }]
    }
  ]
}
